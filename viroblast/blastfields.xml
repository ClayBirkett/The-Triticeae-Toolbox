<?xml version="1.0" encoding="UTF-8"?>
<blast name="blast">
    <obj name="result" prefix="">
        <el>query_name</el>
        <el>query_description</el>
        <el>query_length</el>
        <el>num_hits</el>
        <el>database_name</el>
        <el>algorithm</el>
    </obj>
    <obj name="hit" prefix="hit_">
        <el>name</el>
        <el>description</el>
        <el>length</el>
        <el>num_hsps</el>
    </obj>
    <obj name="hsp" prefix="hsp_">
        <el>evalue</el>
        <el>score</el>
        <el>num_identical</el>
        <el>num_conserved</el>
        <el>gaps</el>
        <el>bits</el>
        <el>percent_identity</el>
        <el>rank</el>
        <el>query_string</el>
        <el>homology_string</el>
        <el>hit_string</el>
        <category name="length" postfix="_length" type="methpar">
            <el>total</el>
            <el>hit</el>
            <el>query</el>
        </category>
        <category name="start" postfix="_start" type="methpar">
            <el>hit</el>
            <el>query</el>
        </category>
        <category name="end" postfix="_end" type="methpar">
            <el>hit</el>
            <el>query</el>
        </category>
        <category name="frame" postfix="_frame" type="field">
            <el>hit</el>
            <el>query</el>
        </category>
        <category name="strand" postfix="_strand" type="field">
            <el>hit</el>
            <el>query</el>
        </category>
    </obj>
</blast>
